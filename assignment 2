l = []
Even = []
Odd = []
 
n = int(input("How many elements in list :- "))
 
for i in range(1, n + 1):
    value = int(input("Please enter the Value of %d Element :- " %i))
    l.append(value)
 
for j in range(n):
    if(l[j] % 2 == 0):
        Even.append(l[j])
    else:
        Odd.append(l[j])
 
print("\n Element in Even List is : ", Even,Odd)
